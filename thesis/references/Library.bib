file = {:C$\backslash$:/Users/wndai/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Phung et al. - 2017 - Enhanced discrete particle swarm optimization path planning for UAV vision-based surface inspection.pdf:pdf},
issn = {09265805},
journal = {Automation in Construction},
keywords = {Bridge inspection,Infrastructure monitoring,Particle swarm optimization,Path planning,Unmanned aerial vehicle,Vision-based inspection},
number = {April},
