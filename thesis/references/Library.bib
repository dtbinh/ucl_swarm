@article{KramerJ.2007,
abstract = {Robotic Development Environments (RDEs) have come to play an increasingly$\backslash$nimportant role in robotics research in general, and for the development$\backslash$nof architectures for mobile robots in particular. Yet, no systematic$\backslash$nevaluation of available RDEs has been performed; establishing a comprehensive$\backslash$nlist of evaluation criteria targeted at robotics applications is$\backslash$ndesirable that can subsequently be used to compare their strengths$\backslash$nand weaknesses. Moreover, there are no practical evaluations of the$\backslash$nusability and impact of a large selection of RDEs that provides researchers$\backslash$nwith the information necessary to select an RDE most suited to their$\backslash$nneeds, nor identifies trends in RDE research that suggest directions$\backslash$nfor future RDE development. This survey addresses the above by selecting$\backslash$nand describing nine open source, freely available RDEs for mobile$\backslash$nrobots, evaluating and comparing them from various points of view.$\backslash$nFirst, based on previous work concerning agent systems, a conceptual$\backslash$nframework of four broad categories is established, encompassing the$\backslash$ncharacteristics and capabilities that an RDE supports. Then, a practical$\backslash$nevaluation of RDE usability in designing, implementing, and executing$\backslash$nrobot architectures is presented. Finally, the impact of specific$\backslash$nRDEs on the field of robotics is addressed by providing a list of$\backslash$npublished applications and research projects that give concrete examples$\backslash$nof areas in which systems have been used. The comprehensive evaluation$\backslash$nand comparison of the nine RDEs concludes with suggestions of how$\backslash$nto use the results of this survey and a brief discussion of future$\backslash$ntrends in RDE design. {\textcopyright} Springer Science+Business Media, LLC 2007.},
