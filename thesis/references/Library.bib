@article{James2018,
abstract = {Particle swarm optimization (PSO) is a heuristic search technique that iteratively im-proves a set of candidate solutions given an objective measure of fitness (Kennedy and Eberhart 1995b). Although vanilla implementations of PSO can be found in some Python evolutionary algorithm toolboxes (Fortin et al. 2012; Biscani, Izzo, and M{\"{a}}rtens 2017), a PSO-specific library that focuses on the said technique is still an open challenge. PySwarms is a research toolkit for Particle Swarm Optimization (PSO) that provides a set of class primitives useful for solving continuous and combinatorial optimization problems. It follows a black-box approach, solving optimization tasks with few lines of code, yet allows a white-box framework with a consistent API for rapid prototyping of non-standard swarm models. In addition, benchmark objective functions and parameter-search tools are included to evaluate and improve swarm performance. It is intended for swarm intelligence researchers, practitioners, and students who would like a high-level declarative interface for implementing PSO in their problems.},
author = {{James V. Miranda}, Lester},
doi = {10.21105/joss.00433},
file = {:C$\backslash$:/Users/wndai/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/James V. Miranda - 2018 - PySwarms a research toolkit for Particle Swarm Optimization in Python.pdf:pdf},
issn = {2475-9066},
journal = {The Journal of Open Source Software},
number = {21},
pages = {433},
title = {{PySwarms: a research toolkit for Particle Swarm Optimization in Python}},
url = {http://joss.theoj.org/papers/10.21105/joss.00433},
volume = {3},
year = {2018}
}
@article{Dorigo1997,
abstract = {This paper introduces ant colony system, a distributed algorithm that is applied to the traveling saleman problem. In ACS, a set of cooperating agents called ants cooperate to find good solutions to TSPs. Ants cooperate using an indirect form of communication mediated by pheromone they deposit on the edges of the TSP graph while building solutions. We study ACS by running experiments to understand its operation. The results show that ACS outperforms other nature-insired algorithms such as simulated annealing and evolutionary computation, and we conclude comparing ACS-3-opt, a version of ACS augmented with a local search procedure, to some of the best performing algorithms for symmetric and asymmetric TSPs.},
