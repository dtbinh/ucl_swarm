journal = {2006 Proceedings of the 23rd International Symposium on Robotics and Automation in Construction, ISARC 2006},
keywords = {Behaviour-based control,Construction site automation,Multi-vehicle coordination,Particle swarm optimization},
pages = {840--845},
title = {{Particle swarm optimization-based coordination of a group of construction vehicles}},
